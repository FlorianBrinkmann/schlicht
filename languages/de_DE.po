msgstr "Änderungen speichern"

#: am-license-menu.php:338
msgid "Key Status"
msgstr "Status des Schlüssels"

#: am-license-menu.php:342
msgid "Key"
msgstr "Schlüssel"

#: am-license-menu.php:346
msgid "Email"
msgstr "E-Mail-Adresse"

#: am-license-menu.php:355
msgid "Deactivation"
msgstr "Deaktivierung"

